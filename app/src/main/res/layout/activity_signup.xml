<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".signup">

    <ScrollView
        android:layout_width="543dp"
        android:layout_height="992dp"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="5dp"
        android:background="#4DAE6395"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/signuptxt"
                android:layout_width="119dp"
                android:layout_height="38dp"
                android:layout_marginStart="32dp"
                android:layout_marginLeft="32dp"
                android:layout_marginTop="128dp"
                android:fontFamily="sans-serif-black"
                android:text="@string/signuptxt"
                android:textColor="@color/black"
                android:textSize="30sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="188dp"
                android:layout_height="165dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="188dp"
                android:layout_marginRight="188dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/catering" />

            <EditText
                android:id="@+id/name_id"
                android:layout_width="327dp"
                android:layout_height="110dp"
                android:layout_marginStart="28dp"
                android:layout_marginLeft="28dp"
                android:layout_marginTop="180dp"
                android:layout_marginEnd="70dp"
                android:layout_marginRight="70dp"
                android:layout_marginBottom="364dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="@string/name_id"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />

            <EditText
                android:id="@+id/addressid"
                android:layout_width="331dp"
                android:layout_height="110dp"
                android:layout_marginStart="28dp"
                android:layout_marginLeft="28dp"
                android:layout_marginEnd="50dp"
                android:layout_marginRight="50dp"
                android:ems="10"
                android:inputType="textPostalAddress"
                android:text="@string/addressid"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.292" />

            <EditText
                android:id="@+id/phone_id"
                android:layout_width="331dp"
                android:layout_height="110dp"
                android:layout_marginStart="28dp"
                android:layout_marginLeft="28dp"
                android:layout_marginEnd="70dp"
                android:layout_marginRight="70dp"
                android:layout_marginBottom="540dp"
                android:ems="10"
                android:inputType="phone"
                android:text="@string/phone_id"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/email_id"
                android:layout_width="333dp"
                android:layout_height="110dp"
                android:layout_marginStart="28dp"
                android:layout_marginLeft="28dp"
                android:layout_marginEnd="70dp"
                android:layout_marginRight="70dp"
                android:ems="10"
                android:inputType="textEmailAddress"
                android:text="@string/email_id"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="436dp" />

            <TextView
                android:id="@+id/passwrd_txt"
                android:layout_width="336dp"
                android:layout_height="20dp"
                android:layout_marginStart="28dp"
                android:layout_marginLeft="28dp"
                android:layout_marginEnd="50dp"
                android:layout_marginRight="50dp"
                android:text="@string/passwrd_txt"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.031"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="562dp" />

            <EditText
                android:id="@+id/passwrd_id"
                android:layout_width="331dp"
                android:layout_height="35dp"
                android:layout_marginStart="28dp"
                android:layout_marginLeft="28dp"
                android:layout_marginEnd="79dp"
                android:layout_marginRight="79dp"
                android:ems="10"
                android:inputType="textPassword"
                android:text="@string/passwrd_id"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="594dp" />

            <TextView
                android:id="@+id/confpw_txt"
                android:layout_width="341dp"
                android:layout_height="48dp"
                android:text="@string/confpw_txt"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="@+id/passwrd_id"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/passwrd_id"
                tools:layout_editor_absoluteY="645dp" />

            <EditText
                android:id="@+id/confpw_id"
                android:layout_width="334dp"
                android:layout_height="46dp"
                android:ems="10"
                android:inputType="textPassword"
                android:text="@string/confpw_id"
                tools:layout_editor_absoluteX="25dp"
                tools:layout_editor_absoluteY="668dp" />

            <CheckBox
                android:id="@+id/agreechckbx_id"
                android:layout_width="339dp"
                android:layout_height="49dp"
                android:text="@string/agreechckbx_id"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="@+id/confpw_id"
                app:layout_constraintStart_toStartOf="@+id/confpw_id"
                tools:layout_editor_absoluteY="734dp" />

            <Button
                android:id="@+id/continue_btn"
                android:layout_width="276dp"
                android:layout_height="51dp"
                android:text="@string/continue_btn"
                app:backgroundTint="@android:color/holo_red_light"
                tools:layout_editor_absoluteX="48dp"
                tools:layout_editor_absoluteY="815dp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="148dp"
                android:layout_height="26dp"
                android:text="@string/textView7"
                android:textSize="18sp"
                tools:layout_editor_absoluteX="64dp"
                tools:layout_editor_absoluteY="913dp" />

            <Button
                android:id="@+id/signin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:text="@string/signin"
                android:textColor="@android:color/holo_red_light"
                app:backgroundTint="@color/white"
                app:layout_constraintBaseline_toBaselineOf="@+id/textView7"
                tools:layout_editor_absoluteX="234dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>